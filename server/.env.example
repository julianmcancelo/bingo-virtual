# Backend environment variables for MySQL
# Copy this file to .env (same folder) and fill the values

# --- Server ---
NODE_ENV=production
PORT=3000
FRONTEND_URL=https://bingo-aled3.vercel.app
JWT_SECRETO=your_jwt_secret_here

# --- MySQL ---
DB_HOST=your_mysql_host
DB_PORT=3306
DB_USER=your_mysql_user
DB_PASSWORD=your_mysql_password
DB_NAME=your_mysql_database

# Optional SSL (enable if your hosting requires MySQL SSL)
DB_SSL=false
DB_SSL_REJECT_UNAUTHORIZED=false
