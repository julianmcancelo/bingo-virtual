{
  "version": 2,
  "builds": [
    {
      "src": "dist/bingo-virtual/server/server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/bingo-virtual/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/bingo-virtual/browser/assets/$1"
    },
    {
      "src": "/(.*\\..*)",
      "dest": "/dist/bingo-virtual/browser/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/bingo-virtual/server/server.mjs"
    }
  ]
}
